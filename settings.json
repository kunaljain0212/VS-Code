{
  "editor.mouseWheelZoom": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "python": "python3 $fullFileName"
  },
  "C_Cpp.updateChannel": "Insiders",
  "git.suggestSmartCommit": true,
  "git.confirmSync": false,
  "files.autoSave": "afterDelay",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "tabnine.experimentalAutoImports": true,
  "editor.fontFamily": "Fira Code",
  "editor.wordWrap": "on",
  "editor.fontLigatures": true,
  "stories.username": "poor_Carlynn",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.largeFileOptimizations": false,
  "git.autofetch": true,
  "editor.codeActionsOnSave": null,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "italic font",
        "scope": [
          "comment",
          "keyword",
          "storage",
          "keyword.control",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.operator.new",
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.default",
          "keyword.control.trycatch",
          "keyword.control.conditional",
          "storage.type",
          "storage.type.class",
          "storage.modifier.tsx",
          "storage.type.function",
          "storage.modifier.async",
          "variable.language",
          "variable.language.this",
          "variable.language.super",
          "meta.class",
          "meta.var.expr",
          "constant.language.null",
          "support.type.primitive",
          "entity.name.method.js",
          "entity.other.attribute-name",
          "punctuation.definition.comment",
          "text.html.basic entity.other.attribute-name",
          "tag.decorator.js entity.name.tag.js",
          "tag.decorator.js punctuation.definition.tag.js",
          "source.js constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {},
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "workbench.preferredLightColorTheme": "One Dark Pro",
  "oneDarkPro.editorTheme": "oneDarkProFlat",
  "oneDarkPro.bold": true,
  "oneDarkPro.vivid": true,
  "dart.previewLsp": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.sideBar.location": "right",
  "editor.renderControlCharacters": false,
  "emmet.excludeLanguages": ["markdown"],
  "[python]": {
    "gitlens.codeLens.symbolScopes": ["!Module"]
  },
  "python.pythonPath": "/bin/python3",
  "python.formatting.provider": "autopep8",
  "code-runner.languageIdToFileExtensionMap": {
    "bat": ".bat",
    "powershell": ".ps1",
    "typescript": ".ts"
  },
  "python.languageServer": "Pylance",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.scopeLineCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {#fff}",
    "opacity: 0.5"
  ],
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": true,
  "workbench.colorTheme": "codeSTACKr Theme",
  "window.titleBarStyle": "custom",
  "editor.cursorBlinking": "expand",
  "workbench.editor.untitled.hint": "hidden",
  "terminal.integrated.rendererType": "dom",
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    {
      "text": "NOTE:",
      "color": "#ff0000",
      "backgroundColor": "pink",
      "overviewRulerColor": "grey"
    },
    {
      "text": "DOCUMENTATION:",
      "color": "#000",
      "isWholeLine": false
    },
    {
      "text": "TODO:",
      "color": "red",
      // "border": "1px solid red",
      // "borderRadius": "2px", //NOTE: using borderRadius along with `border` or you will see nothing change
      "backgroundColor": "orange"
      //other styling properties goes here ...
    }
  ],
  "todohighlight.include": [

    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "window.zoomLevel": 1,
  "window.menuBarVisibility": "compact",
  "editor.fontWeight": null
}
